    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Current File+libs",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "purpose": [
                "debug-test"
            ],
            "stopOnEntry": true,
            "justMyCode": false
        },
        {
            "name": "Server Langchain",
            "type": "debugpy",
            "request": "launch",
            "program": "./app/server.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
            // "cwd": "${workspaceFolder}/api"
        }
    ]
}
